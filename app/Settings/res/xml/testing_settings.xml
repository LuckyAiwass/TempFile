    <PreferenceScreen
            android:title="@string/testing_wifi_info" >
        <intent
                android:action="android.intent.action.MAIN"
                android:targetPackage="com.android.settings"
                android:targetClass="com.android.settings.Settings$WifiInfoActivity" />
    </PreferenceScreen>
    <PreferenceScreen
            android:title="@string/testing_factory" >
        <intent
                android:action="android.intent.action.MAIN"
                android:targetPackage="com.ubx.factorykit"
                android:targetClass="com.ubx.factorykit.Framework.Framework" />
    </PreferenceScreen>

    <!--urovo add *#1261*# -->
